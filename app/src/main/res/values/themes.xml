<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Batech_1" parent="Theme.MaterialComponents.DayNight.NoActionBar">

        <item name="fontFamily">@font/anekdevanagari</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/custom_black_off</item>
        <item name="colorPrimaryDark">@color/custom_orange</item>
        <item name="colorAccent">@color/custom_black_off</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/custom_yellow</item>
        <item name="colorSecondaryVariant">@color/custom_orange</item>
        <item name="colorOnSecondary">@color/custom_black_off</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#f6ae2d</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="navigationTheme">
        <item name="colorPrimary">@color/custom_yellow</item>
        <item name="colorControlHighlight">@color/custom_black_off</item>
    </style>


    <style name = "MyNewDialog" >
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:backgroundDimEnabled">true</item>

    </style >
    <style name="AppTheme.AppBarOverlay" parent="Theme.MaterialComponents.DayNight.NoActionBar" />
</resources>